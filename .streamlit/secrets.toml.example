# Example secrets.toml file for Streamlit Cloud
# This file shows what secrets need to be configured in the Streamlit Cloud dashboard
# Do not include actual API keys in this file

# AssemblyAI API key - Get one at https://www.assemblyai.com/
ASSEMBLYAI_API_KEY = "your_assemblyai_api_key"

# OpenAI API key - Get one at https://platform.openai.com/api-keys
OPENAI_API_KEY = "your_openai_api_key"

# JWT Secret Key - Used for user authentication
JWT_SECRET = "your_jwt_secret_key_at_least_32_characters_long"

# Default admin credentials (CHANGE THESE IN PRODUCTION!)
ADMIN_USERNAME = "admin"
ADMIN_PASSWORD = "admin123" 